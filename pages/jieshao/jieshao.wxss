/* pages/jieshao/jieshao.wxss */
.appBody{
  width: 715rpx;
  margin: 0 auto;
  background: #f4f4f4;
  min-height: 100%;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.YMD{
  width: 690rpx;
  height: 467rpx;
  margin: 0 auto;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.YMD:nth-last-of-type(1){
  margin-bottom: 0;
}
.YMDTxt{
  height: 68rpx;
  font-size:28.96rpx;
  display:flex;
  align-items: center;
  width: 634rpx;
  padding: 0 28rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.YMDBox{
  width: 646rpx;
  height: 160rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #f4f4f4;
  border-radius: 10rpx;
  margin: 20rpx auto 0;
}
.YMDList{
  width: 150rpx;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: column;
  font-size: 25.63rpx;
  color: #636878;
  border-left: 2rpx solid #dee0e9;
}
.YMDList:nth-of-type(1){
  border: none;
}
.YMDListTxt1{
  position: relative;
  top: -10rpx;
}
.YMDList1{
  border: none;
}